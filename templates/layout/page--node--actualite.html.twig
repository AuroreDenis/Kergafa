<div class="layout-container not-frontpage">

    <header role="banner">
        {{ page.header }}
    </header>

    <main role="main">
        <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}
        <div class="layout-content">
            {{ page.content }}
        </div>{# /.layout-content #}

        <div class="map-content">
            {{ page.map }}
        </div>

    </main>

    <footer role="contentinfo">
        <div class="wrapper">
            {{ page.footer }}
        </div>
    </footer>

</div>{# /.layout-container #}
